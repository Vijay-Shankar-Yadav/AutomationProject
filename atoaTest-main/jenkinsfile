pipeline {
    agent any

    stages {
        stage('Start') {
            steps {
                echo 'Pipeline execution started!'
                
            }
        }

        stage('Build') {
            steps {
                echo 'Building the project...'
                bat "mvn clean"
            }
        }

        stage('Test') {
            steps {
                echo 'Running tests...'
                bat "mvn test"
            }
        }

          stage('Compile') {
            steps {
                echo 'compliing the application...'
                bat "mvn compile"
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying the application...'
            }
        }

        stage('End') {
            steps {
                echo 'Pipeline execution completed!'
            }
        }
    }
}
